// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserImage {
  id        String   @default(cuid()) @id
  path      String
  name      String
  createdAt DateTime @default(now())
}
